@model IEnumerable<MvcBasic.DataBase.Entity.Member>
@{
    ViewBag.Title = "List";
}

<h2>List</h2>
<table class="table">
    <tr>
        <th>指名</th><th>メールアドレス</th><th>誕生日</th><th>既婚</th><th>備考</th>
    </tr>
    @foreach(var member in Model)
    {
        <tr>
            <td>@member.Name</td>
            <td>@member.Email</td>
            <td>@member.Birth</td>
            <td>@member.Married</td>
            <td>@member.Memo</td>
        </tr>
    }
</table>

